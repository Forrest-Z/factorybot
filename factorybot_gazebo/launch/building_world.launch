<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find agv_gazebo)/worlds/building.world"/>
    <arg name="verbose" value="true"/>
  </include>

  <include file="$(find factorybot_gazebo)/launch/spawn_agv.launch">
    <arg name="robot_name" value="AGV_2Dlidar"/>
    <arg name="x" value="-10"/>
    <arg name="y" value="-12"/>
    <arg name="xarco_robot_file" value="$(find agv_description)/urdf/agv.urdf.xacro"/>
    <arg name="visualize_lidar2D" value="true"/>
  </include>

  <include file="$(find factorybot_gazebo)/launch/spawn_agv.launch">
    <arg name="robot_name" value="AGV_3Dlidar"/>
    <arg name="x" value="2.7"/>
    <arg name="y" value="10"/>
    <arg name="yaw" value="1.5707"/>
    <arg name="xarco_robot_file" value="$(find agv_description)/urdf/agv.urdf.xacro"/>
    <arg name="visualize_lidar3D" value="true"/>
  </include>
</launch>
